<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/fb_corners" />
            <stroke android:width="1dip" android:color="@color/login_btn_click_color_alpha" />
            <solid android:color="@color/login_btn_click_color_alpha"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/fb_corners" />
            <stroke android:width="1dip" android:color="@color/login_btn_click_color" />
            <solid android:color="@color/login_btn_click_color"/>
        </shape>
    </item>

</selector>