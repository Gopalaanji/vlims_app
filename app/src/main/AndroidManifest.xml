<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="dev.info.basic.viswaLab">

    <permission
        android:name="dev.info.basic.viswaLab.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <application
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity
            android:name=".Activitys.SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.LoginFragmentActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".Activitys.HomeActivity" />
        <activity android:name=".Activitys.WebViewActivity" />
        <!-- GCM Receiver -->
        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="rsr.info.basic.pricedproperty" />
            </intent-filter>
        </receiver>
        <!-- GCM Receiver Service -->
        <service
            android:name=".pushNotificationServices.GCMPushReceiverService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <!-- GCM Registration Intent Service -->
        <service
            android:name=".pushNotificationServices.GCMRegistrationIntentService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>

        <intent-filter>
            <action android:name="com.google.android.c2dm.intent.RECEIVE" />
        </intent-filter>

        <activity android:name=".Activitys.LubeOilActivity" />
        <service
            android:name=".services.TimeService"
            android:enabled="true"></service>
    </application>

</manifest>